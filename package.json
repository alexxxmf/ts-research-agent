{
  "name": "ts-research-agent",
  "version": "1.0.0",
  "description": "Deep research agent powered by SearXNG, Jina.ai, and OpenRouter with configurable depth levels",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx ./examples/basic-usage.ts",
    "example:basic": "tsx ./examples/basic-usage.ts basic",
    "example:advanced": "tsx ./examples/basic-usage.ts advanced",
    "example:custom": "tsx ./examples/basic-usage.ts custom",
    "example:features": "tsx ./examples/advanced-features.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "fetch-models": "tsx ./scripts/fetch-models.ts",
    "searxng:start": "./start-searxng.sh",
    "searxng:stop": "docker-compose down",
    "searxng:logs": "docker-compose logs -f searxng",
    "searxng:restart": "docker-compose restart"
  },
  "keywords": [
    "research",
    "agent",
    "searxng",
    "jina",
    "openrouter",
    "llm",
    "deep-research",
    "web-scraping",
    "ai",
    "typescript"
  ],
  "author": "Alex MF",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexxxmf/research-agent"
  },
  "bugs": {
    "url": "https://github.com/alexxxmf/research-agent/issues"
  },
  "homepage": "https://github.com/alexxxmf/research-agent#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3"
  },
  "peerDependencies": {},
  "ts-node": {
    "esm": true
  }
}
